{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://miso.ai/schemas/bench_result/v1.0.0",
  "title": "MISO Benchmark Result Schema",
  "description": "Schema for individual benchmark test results",
  "type": "object",
  "required": [
    "test_name",
    "status", 
    "execution_time_ms",
    "timestamp",
    "schema_version"
  ],
  "properties": {
    "schema_version": {
      "type": "string",
      "pattern": "^v\\d+\\.\\d+\\.\\d+$",
      "description": "Schema version identifier"
    },
    "test_name": {
      "type": "string",
      "minLength": 1,
      "description": "Unique identifier for the test"
    },
    "status": {
      "type": "string",
      "enum": ["passed", "failed", "skipped", "error"],
      "description": "Test execution status"
    },
    "execution_time_ms": {
      "type": "number",
      "minimum": 0,
      "description": "Test execution time in milliseconds"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp of test execution"
    },
    "accuracy": {
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "description": "Test accuracy percentage (0-100)"
    },
    "samples_processed": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of samples processed in this test"
    },
    "error_message": {
      "type": "string",
      "description": "Error details if status is 'error' or 'failed'"
    },
    "metadata": {
      "type": "object",
      "description": "Additional test-specific metadata"
    }
  },
  "additionalProperties": false
}
